---
import Prose from "@/components/Content/Prose.astro";
import SidebarPage from "@/layouts/SidebarPage.astro";
import { getEntry } from "astro:content";
import "../styles/changelog.css";

const entry = await getEntry("misc", `en/changes`);
if (!entry) return;
const { Content } = await entry.render();
---

<SidebarPage
    sidebarMode="guides"
    meta={{
        title: "KAPLAY.js, Changelog",
        description: "KAPLAY is a JavaScript game library that makes it easy to create games. Is the successor of KABOOM.JS.",
        keywords: "kaplay, KAPLAY, changelog",
    }}
>
    <Prose>
        <Content />
    </Prose>
</SidebarPage>
