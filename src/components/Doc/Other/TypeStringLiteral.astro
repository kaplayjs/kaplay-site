---
import type { DataTypeProps } from "../Utils/types.ts";

type TypeDataStringLiteral = {
    kind: "StringLiteral";
    text: string;
};

interface Props extends DataTypeProps {
    data: TypeDataStringLiteral;
}

const { data } = Astro.props;
---

<span data-dg-string-literal>"{data.text}"</span>
