---
import DocFor from "../DocFor.astro";
import JSDoc from "../JSDoc.astro";
import type { DataTypeParameter } from "../Other/Parameter.astro";
import Parameters from "../Other/Parameters.astro";
import Strong from "../Other/Strong.astro";
import TypeBadge from "../Other/TypeBadge.astro";
import Title from "../Utils/Title.astro";
import type { DataMaybeMemberTypeProps } from "../Utils/types.ts";

type TypeDataPropertySignature = {
    kind: "MethodSignature";
    id: number;
    name: string;
    parameters: DataTypeParameter[];
    type: any[];
};

interface Props extends DataMaybeMemberTypeProps {
    data: TypeDataPropertySignature;
}

const { data, depth, parent } = Astro.props;
---

<TypeBadge depth={depth}>Function</TypeBadge>
<Title name={data.name} parent={parent} class="inline" depth={depth} /><Strong depth={depth}
    >(<Parameters data={data.parameters} depth={depth} />): <DocFor data={data.type} depth={depth} parent={data.name} /></Strong
>

<JSDoc data={data} />
