---
import DocFor from "../DocFor.astro";
import JSDoc from "../JSDoc.astro";
import Parameters from "../Other/Parameters.astro";
import Title from "../Utils/Title.astro";
import type { JSX } from "astro/jsx-runtime";

type TypeDataPropertySignature = {
    kind: "MethodSignature";
    id: number;
    name: string;
    parameters: any[];
    type: any[];
};

type Props = {
    data: TypeDataPropertySignature;
    depth: number;
};

const { data, depth } = Astro.props;
---

<Title class="inline" depth={depth}>{data.name}</Title><strong role={"doc-subtitle" as JSX.AriaRole}
    >(<Parameters parameters={data.parameters} />) => <DocFor data={data.type} depth={depth} /></strong
>
<JSDoc data={data} />
