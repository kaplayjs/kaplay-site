---
import DocFor from "../DocFor.astro";
import JSDoc from "../JSDoc.astro";
import Title from "../Utils/Title.astro";
import type { DataTypeProps } from "../Utils/types.ts";

type TypeDataFunctionDeclaration = {
    kind: "FunctionDeclaration";
    id: number;
    name: string;
    parameters: [];
    type: any[];
    questionToken: boolean;
};

interface Props extends DataTypeProps {
    data: TypeDataFunctionDeclaration;
}

const { data, depth } = Astro.props;
---

<span data-dg-function-declaration><Title depth={depth}>{data.name}:</Title> <DocFor data={data.type} depth={depth} /></span>

<JSDoc data={data} />
